digraph CNN_Object_Detection {
    rankdir=TB;
    node [shape=box, style=rounded];

    input [label="Input Image"];
    conv1 [label="Convolutional Layer 1"];
    pool1 [label="Pooling Layer 1"];
    conv2 [label="Convolutional Layer 2"];
    pool2 [label="Pooling Layer 2"];
    flatten [label="Flatten"];
    fc1 [label="Fully Connected Layer 1"];
    fc2 [label="Fully Connected Layer 2"];
    output [label="Output: Bounding Boxes & Class Scores"];

    input -> conv1;
    conv1 -> pool1;
    pool1 -> conv2;
    conv2 -> pool2;
    pool2 -> flatten;
    flatten -> fc1;
    fc1 -> fc2;
    fc2 -> output;
}
